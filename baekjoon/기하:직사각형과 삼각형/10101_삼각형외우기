#include <iostream>
using namespace std;

int main(){
    int a, b, c;
    cin >> a >> b >> c;
    if((a+b+c) != 180)
        cout << "Error";
    else{
        if(a == b){
            if(b==c){
                cout << "Equilateral";
            }
            else
                cout << "Isosceles";
        }
        else if(a == c){
            if(b==c){
                cout << "Equilateral";
            }
            else
                cout << "Isosceles";
        }
        else if(c == b){
            if(a==c){
                cout << "Equilateral";
            }
            else
                cout << "Isosceles";
        }
        else 
            cout << "Scalene";
        }
}
