#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main(){
    int n, i, count;
    string s;
    cin >> n;
    count = 0;
    i = 665;
    while(count != n){
        s = to_string(i);
        if(s.find("666") != string::npos){
            count++;
        }
        i++;
    }
    cout << --i;
}
