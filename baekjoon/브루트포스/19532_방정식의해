#include <iostream>
#include <vector>
using namespace std;

int main(){
    int a, b, c, d, e, f, x, y, x_sol, y_sol;
    cin >> a >> b >> c >> d >> e >> f;
    
    for(x = -999; x<=999; x++){
        for(y = -999; y <=999; y++){
            if((a*x + b*y ==c) && (d*x + e*y ==f)){
                x_sol = x;
                y_sol = y;
                break;
            }
        }
    }
    cout << x_sol << " " << y_sol;
}
