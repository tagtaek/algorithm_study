#include <iostream>
using namespace std;

int main(){
    int n;
    cin >> n;
    cout << n << '\n' << 1;
    //최고차항의 차수: n 이므로 1
}

/* 코드
MenOfPassion(A[], n) {
    sum <- 0;
    for i <- 1 to n                 // i~n 까지 반복
        sum <- sum + A[i]; # 코드1   // sum에 A의 i번째 요소를 더한다.
    return sum;
}
// 입력의 크기 n에 대하여 반복문을 n번 수행 
수행횟수: n
최고차항의 차수: 1
*/
