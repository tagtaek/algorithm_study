#include <iostream>
using namespace std;

int main(){
    long n;  //입력되는 n의 범위를 고려하자. n <= 500,000  이므로 int가 아닌 long으로 선언필요!!
    cin >> n;
    cout << n*n << '\n' << 2;
    //최고차항의 차수: n 이므로 1
}


/* 코드
MenOfPassion(A[], n) {
    sum <- 0;
    for i <- 1 to n               //i를 1~n 반복
        for j <- 1 to n           //각 i에 대하여 j를 1~n 반복
            sum <- sum + A[i] × A[j]; # 코드1 //영향 x
    return sum;
}
*/
