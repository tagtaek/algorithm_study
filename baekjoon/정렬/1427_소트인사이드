#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;

int main(){
    string s;
    vector<char> c;
    cin >> s;
    for(int i=0; i<s.length(); i++){
        c.push_back(s[i]);
    }
    
    sort(c.begin(), c.end());

    for(int i=c.size()-1; i>=0; i--){
        cout << c[i];
    }

}
