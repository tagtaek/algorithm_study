#include <iostream>
#include <string>
using namespace std;
int main(){
    string str;
    char arr[5][15];
    for(int i=0; i<5;i++){
        cin >> str;
        for(int j=0; j<15;j++){
            if(j<str.length())
                arr[i][j] = str[j];
            else
                arr[i][j] = NULL;           
        }
    }
    for(int i=0;i<15;i++){
        for(int j=0;j<5;j++){
            if(arr[j][i] != NULL){ //null이면 출력하지 않도록 해야한다! 문자 NULL(= '\0')은 정수형 0 이다.
            cout << arr[j][i];
            }
        }
    }
}
