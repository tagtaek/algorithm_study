#include <iostream>
using namespace std;

int main(){
    int n, m;
    cin >> n >> m;
    int basket[n];
    for (int i=0; i<n; i++)
        basket[i] = 0;
    int start, end, num;
    
    for(int i=0; i<m; i++){
        cin >> start >> end >> num;
        for(int j= start-1; j<end; j++){
                basket[j] = num;
        }
    }
    for(int i=0; i<n; i++){
        cout << basket[i] << " ";
    }
}
