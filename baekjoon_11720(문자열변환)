#include <iostream>
#include <string>
using namespace std;

int main(){
    long long n, sum;
    long long num;
    string str;
    str = "";
    cin >> n >> str;
    sum = 0;
    num = strtoll(str);
    for(int i=0; i<n; i++){
        sum += num%10;
        num = num/10;
    }

    cout << sum;
}
