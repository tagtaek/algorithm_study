#include <iostream>
#include <string>
using namespace std;
int main() {
    string s;
    int indexx;
    char item = 'a';
    cin >> s;
    while(item <= 'z'){
        for(int i=0; i< s.length();i++){
            if(item == s[i])
                indexx = i;
        }
        if(s.find(item) != string::npos)
            cout <<  indexx << " ";
        else
            cout << -1 << " ";
        item++;
    }

    
}
