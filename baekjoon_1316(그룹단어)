#include <iostream>
#include <string>
using namespace std;

int main(){
    int n;
    int count_group = 0;
    string s;

    for(int i=0; i<n; i++){
        cin >> s;
        char arr[] = {0};
        for(int j=0;j<s.length();){
            for(int k=0; k<j; k++){
                arr[k] = s[j] ;
            }
            
        }
    }
}
