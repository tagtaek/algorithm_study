#include <iostream>
using namespace std;
int main(){
    int n, max;
    cin >> n;
    double score[n]; //배열들을 처음부터 double로 선언했더니 정답이 나왔다.
    
    for(int i=0; i<n; i++){
        cin >> score[i];
    }
    max = score[0];
    for(int i=0;i<n;i++){
        if(max > score[i])
            max = max;
        else
            max = score[i];
    }
    double newscore[n];
    double newsum;
    double newave;
    newsum = 0;
    for(int i=0;i<n;i++){
        newscore[i] = (score[i]/max)*100;
        newsum += newscore[i];
    }
    newave = newsum/n;
    cout.precision(4);
    cout << newave;
}
