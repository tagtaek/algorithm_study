#include <iostream>

using namespace std;

int main(){
    int n1, n2, n3, price;
    cin >> n1 >> n2 >> n3;
    
    if ((n1 == n2) && (n2 == n3)){
        price = 10000 + (1000^n1);
        cout << price;
    }
    else if ((n1 == n2) && (n2 != n3)){
        price = 1000 + (n1^100);
        cout << price;
    }
    else if ((n1 != n2) && (n2 == n3)){
        price = 1000 + (n2^100);
        cout << price;
    }
    else if ((n1 == n3) && (n2 != n3)){
        price = 1000 + (n1^100);
        cout << price;
    }
    else{
        if (n1 > n2){
            if(n1>n3)
                cout << (n1^100);
            else
                cout << (n3^100);
        }
        else if (n1 < n2){
            if(n2>n3)
                cout << (n2^100);
            else
                cout << (n3^100);
        }
    }
        
}
