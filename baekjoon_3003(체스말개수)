#include <iostream>
#include <string>
using namespace std;
int main() {
    int arr[6];
    int result[6];
    for(int i=0;i<6;i++){
        cin >> arr[i];
    }
    for(int i=0; i<=1; i++){
        result[i] = 1 - arr[i];
    }
    for(int i=2; i<=4; i++){
        result[i] = 2 - arr[i];
    }
    result[5] = 8 - arr[5];

    for(int i=0;i<6;i++){
        cout << result[i] << " ";
    }
}
